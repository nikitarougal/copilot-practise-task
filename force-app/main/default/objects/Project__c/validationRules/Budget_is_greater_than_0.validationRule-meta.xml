<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Budget_is_greater_than_0</fullName>
    <active>true</active>
    <errorConditionFormula>Budget__c  &lt;= 0</errorConditionFormula>
    <errorDisplayField>Budget__c</errorDisplayField>
    <errorMessage
  >The Budget__c field on the Project__c object is greater than $0</errorMessage>
</ValidationRule>
